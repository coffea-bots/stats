{
  "name": "statsbot",
  "version": "1.0.0-beta1",
  "description": "protocol independent chat statistics bot made with coffea",
  "bin": {
    "bot": "build/index.js"
  },
  "scripts": {
    "start": "npm run start:prod",
    "start:dev": "cross-env NODE_ENV=development DEBUG=bot:* babel-node src/index.js",
    "start:prod": "npm run prepublish && cross-env NODE_ENV=production DEBUG=bot:*,-*debug,-*log node build/index.js",
    "watch": "cross-env NODE_ENV=development DEBUG=bot:* babel-watch -w src src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf build",
    "compile": "babel --presets es2015 -d build/ src/",
    "prepublish": "npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coffea-bots/stats.git"
  },
  "keywords": [
    "coffea",
    "chat",
    "statistics",
    "stats",
    "bot",
    "messaging",
    "slack",
    "irc",
    "telegram"
  ],
  "author": "Daniel Bugl <me@omnidan.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coffea-bots/stats/issues"
  },
  "homepage": "https://github.com/coffea-bots/stats#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-watch": "^2.0.2",
    "cross-env": "^1.0.7",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "coffea": "1.0.0-beta15",
    "debug-dude": "^1.0.3",
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "lowdb": "^0.13.1",
    "node-emoji": "^1.3.1"
  }
}
